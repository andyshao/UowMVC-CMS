
@using UowMVC.Models;
@using UowMVC.SDK;

@model LogViewModel
@{
    ViewBag.Title = "Error";
}
<div class="row" style="margin-top:15px;margin:0;padding:0">
    <div class="col-lg-12">
        <div class="well">
            <div class="text-left">
                <h3>1.基本信息</h3>
            </div>
            <div>
                <table class="table table-condensed">
                    <thead>
                        <tr>
                            <th>名称</th>
                            <th>内容</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>
                                Code
                            </td>
                            <td>
                                @Model.StatusCode
                            </td>
                        </tr>
                        <tr>
                            <td>
                                IP
                            </td>
                            <td>
                                @Model.UserIP
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Status
                            </td>
                            <td>
                                @Model.Status.ToDescription()
                            </td>
                        </tr>
                        <tr>
                            <td>
                                User
                            </td>
                            <td>
                                @Model.UserName
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Action
                            </td>
                            <td>
                                @Model.Action
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Url
                            </td>
                            <td>
                                @Model.URL
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Summary
                            </td>
                            <td>
                                @Model.Message
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Client
                            </td>
                            <td>
                                @Model.Client
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
           
            <div class="text-left">
                <h3>2.Exception</h3>
            </div>
            @if (Model.Exception != null)
            {
                <div><code class="html">@Model.Exception</code></div>
            }
        </div>
    </div>
</div>


